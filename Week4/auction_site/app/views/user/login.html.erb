<h1>Log-In Page</h1>

<form action="/users/login" method="post">

	<div>
	Email:<br>
	  <input type="text" name="email">
	</div>  

	<div>
	Password:<br>
	  <input type="password" name="password">
	</div>

	<input type="submit" value="Submit">

	  <input type="hidden" name="authenticity_token"
      value="<%= form_authenticity_token %>">

</form>


